{
  "name": "gal-digital-gmbh/hashids-bundle",
  "version": "2.0.0",
  "type": "symfony-bundle",
  "description": "Symfony bundle integrating hashids",
  "keywords": [
    "symfony",
    "hashids"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Flack",
      "email": "christian.flack@gal-digital.de"
    },
    {
      "name": "Cayetano Soriano",
      "email": "neoshadybeat@gmail.com"
    }
  ],
  "require": {
    "php": ">=8",
    "hashids/hashids": "^4.1",
    "symfony/framework-bundle": "^5.0|^6.0",
    "symfony/service-contracts": "^2.2|^3.0",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4",
    "phpstan/phpstan": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "GalDigitalGmbh\\HashidsBundle\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
